#include "{TITLE}.hpp"

{TITLE}::{TITLE}() {PARENTCPP}{}

{TITLE}::{TITLE}({TITLE} const & src) {PARENTCPP}{
	*this = src;	
}

{TITLE}::~{TITLE}() {}

{TITLE} & {TITLE}::operator=({TITLE} const & src) {
	if (this != &src)
    {
        *this = src;
    }
	return (*this);
}

